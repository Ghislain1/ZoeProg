{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\ZoeProg\\src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "home/**.md",
          "home/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "**/toc.yml"

        ],
        "src": ".\\ZoeProg.Documentation\\"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "_wwwroot/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [

    ],
    "fileMetadataFiles": [],
    "template": [ "default", "template/darkfx" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}